{
    "name": "Chromakey",
    "version": "0.1",
    "description": "WebSocket control for chrome",
    "manifest_version": 2,
    "background": {
        "scripts": [
            "src/state.js",
            "src/contrib/socket.io.js",
            "src/tabs.js",
            "src/background.js"
        ],
        "persistent": false
    },
    "permissions": [
        "tabs"
    ]
}
